{
	"manifest_version": 2,
	"name": "DeepPast extension",
	"description": "Анализ страницы на наличие устаревших слов",
	"version": "1.0.0",
	"icons": {"128": "icon.png"},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
		  "js": ["dpast.js"]
		}
	],
	"permissions": ["activeTab"]
}
